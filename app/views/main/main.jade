            box-shadow:inset 0 3px 8px rgba(0,0,0,0.125);    
        }

        /* link tag (a)*/
        #left ul.nav li.parent ul li a {
            color: #222;
            border: none;
            display:block;
            padding-left: 5px;    
        }

        #left ul.nav li.parent ul li a:hover {
            background-color: #fff;
            -webkit-box-shadow:none;
            -moz-box-shadow:none;
            box-shadow:none;  
        }

