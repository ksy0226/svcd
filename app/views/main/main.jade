            margin-left: 0px;
        }
        #left ul.nav li a:hover {
            text-decoration: none;
        }

        #left ul.nav li a:hover .lbl {
            color: #999!important;
        }

        #left ul.nav li.current>a .lbl {
            background-color: #999;
            color: #fff!important;
        }

        /* parent item */
        #left ul.nav li.parent a {
            padding: 0px;
            color: #ccc;
        }
        #left ul.nav>li.parent>a {
            border: solid 1px #999;
            text-transform: uppercase;
        }    
