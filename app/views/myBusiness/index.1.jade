block content
      // ==============================================================
      // Start right Content here
      // ==============================================================
      .content-page
        // Start content
        .content
          .container
            // Page-Title
            .row
              .col-sm-12
                .page-title-box
                  ol.breadcrumb.pull-right
                    li
                      a(href='#') Home
                    li
                      a(href='#') SD 담당자
                    li.active 나의 업무지정
                  h4.page-title 나의 업무지정
            .row
              .col-lg-12
                .card-box
                  form.form-horizontal.group-border-dashed(action='#', onsubmit='return false;')
                    .form-group
                      label.col-sm-2.control-label 상위업무 
                      .col-sm-4
                        select.form-control(id='higher_cd' name='higher_cd')
                          option(value ='*') 전체
                          - if (higherprocess.length > 0) {
                            - for( var i = 0 ; length = higherprocess.length > i ; i++ ){ //속도가 느리다고 알고 있음.
                                option(value ='#{higherprocess[i].higher_cd}') #{higherprocess[i].higher_nm}
                            - }
                          - } 
                      .col-sm-2
                        button.btn.btn-primary(id='searchBtn' type='button') 조회
            .row
              .col-sm-12
                .card-box
                  .row
                    .col-sm-12
                      button.btn.btn-primary.waves-effect.waves-light.pull-right(type='button')
                        i.fa.fa-paper-plane.m-r-5
                        | 저장
                  hr.m-t-10.m-b-10
                  .row
                    .col-sm-12
                      table.m-t-5(width='100%')
                        tbody
                          tr
                            td(width='20%')
                              h4.pull-left
                                b 나의 업무
                            td(width='25%')
                              span.pull-right
                                button.btn.btn-xs.btn-inverse.m-r-5(type='button')
                                  i.fa.fa-check-square-o.m-r-5
                                  |  전체선택
                                button.btn.btn-xs.btn-default(type='button')
                                  i.fa.fa-square-o.m-r-5
                                  |  선택취소
                            td(width='10%')
                            td(width='20%')
                              h4.pull-left
                                b 지정할 업무리스트
                            td(width='25%')
                              span.pull-right
                                button.btn.btn-xs.btn-inverse.m-r-5(type='button')
                                  i.fa.fa-check-square-o.m-r-5
                                  |  전체선택
                                button.btn.btn-xs.btn-default(type='button')
                                  i.fa.fa-square-o.m-r-5
                                  |  선택취소
                      select#my_multi_select2.multi-select(name='my_multi_select2[]', data-plugin='multiselect', data-selectable-optgroup='true', multiple='multiple')
                        optgroup(label='그룹웨어')
                          option.p-l-80 게시판
                          option.p-l-80 공통기능
                          option.p-l-80 기타
                          option.p-l-80 메신저
                          option.p-l-80 명함관리
                          option.p-l-80 문서관리
                          option.p-l-80 업무지원
                          option.p-l-80 이메일
                          option.p-l-80 일정관리
                          option.p-l-80 자산관리
                          option.p-l-80 전자결제
                          option.p-l-80 카페
                        optgroup(label='전자펜수리유지보수')
                          option.p-l-80 기타 1
                          option.p-l-80 기타 2


